doctype html
<!--[if IE 8]> <html class="ie ie8" lang="en"> <![endif]-->
<!--[if IE 9]> <html class="ie ie9" lang="en"> <![endif]-->
html.no-js(lang='en')


  head
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    //meta(name='viewport', content='width=device-width,initial-scale=1'
    
    //- Fonts
    link(href='http://fonts.googleapis.com/css?family=Roboto:400,100', rel='stylesheet')
    link(href='http://fonts.googleapis.com/css?family=Ubuntu:400,300', rel='stylesheet')
    //- Primary stylesheet
    link(href='assets/css/salamat-styles.css', rel='stylesheet')
    //- Icon Fonts
    link(href='assets/css/icon-font/elegant-icons.css', rel='stylesheet')
    link(href='assets/css/icon-font/font-awesome.css', rel='stylesheet')
    link(href='assets/css/icon-font/simple-line-icons.css', rel='stylesheet')
    


  body(itemscope, itemtype='http://schema.org/WebPage')


    // HEADER
    header

      .pre-header__shell
        .pre-header
          address.pre-header__section--lt
            
            //- links to social networks
            div
              ul.pre-header__social
                li
                  a.pre-header__link(href='')
                    i.icon-fa.fa-twitter.pre-header__icon
                li
                  a.pre-header__link(href='')
                    i.icon-fa.fa-facebook.pre-header__icon
                li
                  a.pre-header__link(href='')
                    i.icon-fa.fa-google-plus.pre-header__icon

            //- phone
            p
              span.pre-header__info
                i.icon-fa.fa-phone.pre-header__icon
                span +1 (800) 5260 888 52 63

            //- email
            p
              a.pre-header__info.pre-header__link(href='mailto:vargentum@gmail.com')
                i.icon-fa.fa-envelope.pre-header__icon
                span vargentum@gmail.com


          .pre-header__section--rt
            select
              option EN
              option GE
              option UA

            select
              option USD
              option EUR
              option UAH

            a.pre-header__link(href="#") Log In


      .header__shell 
        .header    

          nav.nav__shell
            ul.nav
              li
                a.nav__link(href="#") Home

              li
                a.nav__link(href="#") Portfolio

              li
                a.nav__link(href="#") Services

              li
                a.nav__link(href="#") Blog

              li.has-dropdown.nav__mega-dropdown__shell
                a.nav__link(href="#") Pages
                //- Mega Dropdown
                div.nav__mega-dropdown
                  - var i = 0
                  while i < 3
                    - i++
                    section.nav__mega-dropdown__unit
                      header.nav__mega-dropdown__unit__title  Menu item 
                        = i 
                      ul
                        - var n = 0
                        while n < 5
                          - n++
                          li
                            a.nav__mega-dropdown__unit__link(href="#") Menu item
                              = i
                              = n

                  section.nav__mega-dropdown__unit
                    header.nav__mega-dropdown__unit__title  Custom item
                    img(src="data:image/png;base64,", alt="")          
                    


              li.has-dropdown.nav__dropdown__shell
                a.nav__link(href="#") Shop
                //- Dropdown 1-level
                ul.nav__dropdown
                  li
                    a.nav__dropdown__link(href="#") Menu item 1

                  li.nav__dropdown--sub__shell
                    a.nav__dropdown__link(href="#") Menu item 2
                    //- Dropdown 2-level (right)
                    ul.nav__dropdown--sub.nav__dropdown--sub--R
                      li
                        a.nav__dropdown__link(href="#") Menu item 2.1

                      li
                        a.nav__dropdown__link(href="#") Menu item 2.2

                      li
                        a.nav__dropdown__link(href="#") Menu item 2.3

                  li.nav__dropdown--sub__shell
                    a.nav__dropdown__link(href="#") Menu item 3
                    //- Dropdown 2-level (left)
                    ul.nav__dropdown--sub.nav__dropdown--sub--L
                      li
                        a.nav__dropdown__link(href="#") Menu item 3.1

                      li
                        a.nav__dropdown__link(href="#") Menu item 3.2

                      li.nav__dropdown--sub__shell
                        a.nav__dropdown__link(href="#") Menu item 3.3

                        //- Dropdown 3-level (left)
                        ul.nav__dropdown--sub.nav__dropdown--sub--L
                          li
                            a.nav__dropdown__link(href="#") Menu item 3.3.1

                          li
                            a.nav__dropdown__link(href="#") Menu item 3.3.3

                          li
                            a.nav__dropdown__link(href="#") Menu item 3.3.3



              li
                span.nav__search__label.icon-
                form.nav__search.is-active
                  input.nav__search__input(placeholder="Type Your Request and Press Enter")


              li
                span.nav__shopcart__label
                .nav__shopcart
                  ul.nav__shopcart__list
                    li
                      article.nav__shopcart__unit
                        aside.nav__shopcart__unit
                        div.nav__shopcart__unit__main
                          h3.nav__shopcart__unit__title iHome iDL46GC Lightning Dock Dual
                          footer
                            span.nav__shopcart__unit__quantity 1 x
                            span.nav__shopcart__unit__price $ 299.99
                            span.nav__shopcart__unit__action-delete

                    li
                      article.nav__shopcart__unit
                        aside.nav__shopcart__unit
                        div.nav__shopcart__unit__main
                          h3.nav__shopcart__unit__title Harman Kardon Aura Wireless Speaker System
                          footer
                            span.nav__shopcart__unit__quantity 2 x
                            span.nav__shopcart__unit__price $ 499.99
                            span.nav__shopcart__unit__action-delete





          a.header__logo(href='/')
            i.header__logo__icon
            span Salamat
            


    // CONTENT
    main.l-main

      include pages/main/main

      //section.l-main__column--primary
      //  include typography/typography
      //aside.l-main__column--secondary

    // FOOTER
    footer.l-footer

      small.copyright



    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js')
    script(src='assets/js/salamat-plugins.js')
    script(src='assets/js/salamat-components.js')
